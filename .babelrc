{
    "presets": [[
        "env", {
            "targets": {
                "browsers": ["last 2 Chrome versions"]
            }
        }
    ]],
    "plugins": [
        "transform-class-properties",
        "transform-react-jsx",
        ["transform-es2015-modules-umd", {
            "exactGlobals": true,
            "globals": {
                "index": "ReactHotkeyManager"
            }
        }]
    ]
}
